<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dtstack.taier.dao.mapper.TenantUserMapper">

    <select id="queryTenantIdListByUser" resultType="java.lang.Long">
        select
            tenant_id
        from
            tenant_user
        where
            user_id=#{userId}
    </select>



</mapper>